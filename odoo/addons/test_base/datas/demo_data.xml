<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Test base sub demo many2one-->
        <record id="test_child_many2one_demo" model="test.child.many2one">
            <field name="name">Child Demo M2O</field>
        </record>

        <!-- Test demo many2one -->
        <record id="test_many2one_demo" model="test.many2one">
            <field name="name">Demo M2O</field>
            <field name="child_m2o_id" ref="test_child_many2one_demo"/>
        </record>

        <!-- Test demo many2one for multi value -->
        <record id="test_many2one_demo2" model="test.many2one">
            <field name="name">Demo 2 M2O</field>
            <field name="child_m2o_id" ref="test_child_many2one_demo"/>
        </record>

        <!-- Demo For access rights to group_system and only read rights to group_user -->
        <record id="test_m2o_group_system_demo" model="test.m2o.group.system">
            <field name="name">Demo</field>
        </record>

        <!-- Test demo many2many -->
        <record id="test_many2many_demo" model="test.many2many">
            <field name="name">Demo M2M</field>
        </record>

        <!-- Test base demo Parent one -->
        <record id="test_base_parent1" model="test.base">
            <field name="name">Parent1</field>
            <field name="m2o_group_system_id" ref="test_m2o_group_system_demo"/>
            <field name="many2one_id" ref="test_many2one_demo"/>
            <field name="many2many_ids" eval="[(4, test_many2many_demo)]"/>
            <field name="boolean" eval="True"/>
        </record>

        <!-- Test base demo child one of one -->
        <record id="test_base_child1_1" model="test.base">
            <field name="name">Child1-1</field>
            <field name="m2o_group_system_id" ref="test_m2o_group_system_demo"/>
            <field name="parent_id" ref="test_base_parent1"/>
            <field name="many2one_id" ref="test_many2one_demo"/>
            <field name="many2many_ids" eval="[(4, test_many2many_demo)]"/>
            <field name="boolean" eval="True"/>
        </record>

        <!-- two parent child record for multi record test -->
        <!-- Test base demo Parent two-->
        <record id="test_base_parent2" model="test.base">
            <field name="name">Parent2</field>
            <field name="m2o_group_system_id" ref="test_m2o_group_system_demo"/>
            <field name="many2one_id" ref="test_many2one_demo2"/>
            <field name="boolean" eval="False"/>
        </record>

        <!-- Test base demo child two of one -->
        <record id="test_base_child2_1" model="test.base">
            <field name="name">Child2-1</field>
            <field name="m2o_group_system_id" ref="test_m2o_group_system_demo"/>
            <field name="parent_id" ref="test_base_parent2"/>
            <field name="many2one_id" ref="test_many2one_demo2"/>
            <field name="boolean" eval="False"/>
        </record>

        <!-- Test base demo child two of one -->
        <record id="test_base_belgium" model="test.base">
            <field name="name">Belgium</field>
            <field name="m2o_group_system_id" ref="test_m2o_group_system_demo"/>
            <field name="parent_id" ref="test_base_parent2"/>
            <field name="many2one_id" ref="test_many2one_demo2"/>
            <field name="boolean" eval="False"/>
        </record>

        <!-- Test demo one2many for test base Parent1-->
        <record id="test_one2many_parent1" model="test.one2many">
            <field name="login">Demo O2M of Parent1</field>
            <field name="test_base_id" ref="test_base_parent1"/>
        </record>

        <record id="test_one2many_child1_1" model="test.one2many">
            <field name="login">Demo O2M of child1 of 1</field>
            <field name="test_base_id" ref="test_base_child1_1"/>
        </record>

        <record id="test_one2many_parent2" model="test.one2many">
            <field name="login">Demo O2M of Parent2</field>
            <field name="test_base_id" ref="test_base_parent2"/>
        </record>

        <record id="test_one2many_child2_1" model="test.one2many">
            <field name="login">Demo O2M of child2 of 1</field>
            <field name="test_base_id" ref="test_base_child2_1"/>
        </record>

        <record id="test_one2many_belgium" model="test.one2many">
            <field name="login">Demo O2M of Belgium</field>
            <field name="test_base_id" ref="test_base_belgium"/>
        </record>

    </data>
</odoo>
