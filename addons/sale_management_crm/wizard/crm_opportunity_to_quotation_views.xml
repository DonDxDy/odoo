        <field name="model">crm.quotation.partner</field>
        <field name="priority">10</field>
        <field name="inherit_id" ref="sale_crm.crm_quotation_partner_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//*[@id='new_qoutation_apply_button']" position="replace">
                <button name="action_apply" id="new_qoutation_apply_button" string="Confirm" type="object" class="btn-primary"/>
            </xpath>
        </field>
    </record>
</odoo>
