<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

  <div class="o_dropdown" t-name="wowl.Dropdown"
    t-on-mouseenter.prevent="onTogglerMouseEnter"
    t-on-mouseleave.prevent="onTogglerMouseLeave"
    owl="1">
    <div
      t-on-click.prevent="onTogglerClick"
      t-on-mouseover.prevent="onTogglerMouseOver">
      <t t-slot="default" />
    </div>
    <t t-if="state.show" t-slot="dropdown" />
  </div>

</templates>
