<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="widget_assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/graph_account/static/src/js/demo.js"></script>
        </xpath>
    </template>
    <record model="ir.actions.client" id="graph_action">
        <field name="name">Graph</field>
        <field name="tag">line_chart_template</field>
    </record>

    <menuitem id="root_menu" name="Graph Account" sequence="3"/>
    <menuitem name="Graph" action="graph_action" id="graph_menu" parent="root_menu"/>
</odoo>
