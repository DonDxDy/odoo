<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    	<record id="rule_slide_channels" model="ir.rule">
            <field name="name">Channel: restriction by Is Course</field>
            <field name="model_id" ref="model_slide_channel"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('is_course', '=', False)]</field>
        </record>

        <record id="rule_course_course" model="ir.rule">
            <field name="name">Course: restriction by Is Course</field>
            <field name="model_id" ref="model_course_course"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('is_course', '=', True)]</field>
        </record>

    </data>
</odoo>