<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_account_payment_form_inherit_l10n_in" model="ir.ui.view">
        <field name="name">account.payment.form.inherit.l10n.in</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='journal_id']" position="after">
                <field name="l10n_in_place_of_supply"/>
                <field name="l10n_in_tax_id" groups="base.group_no_one"/>
            </xpath>
         </field>
    </record>

</odoo>
