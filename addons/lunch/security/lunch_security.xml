<?xml version="1.0" ?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_lunch_category">
            <field name="name" eval="'Lunch Category'" />
            <field name="description">
                Helps you handle your lunch needs, if you are a manager you will be able to create new products, cashmoves and to confirm or cancel orders.
            </field>
            <field name="sequence">16</field>
        </record>
        <record id="group_lunch_user" model="res.groups">
            <field name="name" eval="'User Group'" />
            <field name="category_id" ref="module_lunch_category" />
            <field name="users" eval="[(4, ref('base.user_demo'))]" />
        </record>
        <record id="group_lunch_manager" model="res.groups">
            <field name="name" eval="'Manager Group'" />
            <field name="category_id" ref="module_lunch_category" />
            <field name="implied_ids" eval="[(4, ref('group_lunch_user'))]" />
            <field name="users" eval="[(4, ref('base.user_root'))]" />
        </record>
    </data>
</odoo>
