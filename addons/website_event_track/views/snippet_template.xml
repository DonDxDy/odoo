<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_sponsors" name="Sponsors">
        <section class="s_wevent_track_our_sponsors" t-att-data-res-id="res_id" t-att-data-res-model="res_model">
	        <div class="container mt32 mb16 d-print-none">
	            <h2 class="text-center mb32">Our Sponsors</h2>
	            <div class="o_our_sponsors"/>
	        </div>
        </section>
</template>

<template id="website_sponsors_snippet" inherit_id="website.snippets">
    <xpath expr="//t[@id='event_sponsors_hook']" position="replace">
        <t t-snippet="website_event_track.s_sponsors" t-thumbnail="/website_event_track/static/src/img/s_sponsors.png"/>
    </xpath>
</template>

</odoo>
