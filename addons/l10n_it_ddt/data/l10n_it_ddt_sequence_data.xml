<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_l10n_it_ddt" model="ir.sequence">
            <field name="name">DDT</field>
            <field name="code">l10n.it.ddt</field>
            <field name="prefix">DDT/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- carriage conditions -->
        <record id="carriage_condition_PF" model="l10n.it.ddt.type">
            <field name="name">PORTO FRANCO</field>
        </record>
        <record id="carriage_condition_PA" model="l10n.it.ddt.type">
            <field name="name">ASSIGNED PORT</field>
        </record>
        <record id="carriage_condition_PAF" model="l10n.it.ddt.type">
            <field name="name">WITH INVOICE INVOICE</field>
        </record>
    </data>
</odoo>