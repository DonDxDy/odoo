<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_l10n_it_ddt" model="ir.sequence">
            <field name="name">DDT</field>
            <field name="code">l10n.it.ddt</field>
            <field name="prefix">DDT/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- DDT type -->
        <record id="ddt_type_bilaterale" model="l10n.it.ddt.type">
            <field name="name">DDT Bilaterale</field>
        </record>
        <record id="ddt_type_per_terzeparti" model="l10n.it.ddt.type">
            <field name="name">DDT Per Terzeparti</field>
        </record>

        <!--- DDT Tag-->
        <record id="ddt_tag_sale" model="l10n.it.ddt.tag">
            <field name="name">Sale</field>
        </record>
         <record id="ddt_tag_account_vision" model="l10n.it.ddt.tag">
            <field name="name">Account vision</field>
        </record>
         <record id="ddt_tag_deposit_account" model="l10n.it.ddt.tag">
            <field name="name">Deposit account</field>
        </record>
         <record id="ddt_tag_account_sales" model="l10n.it.ddt.tag">
            <field name="name">Account sales</field>
        </record>
    </data>
</odoo>