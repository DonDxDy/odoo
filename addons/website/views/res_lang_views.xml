<?xml version="1.0"?>
<odoo>

<record id="view_lang_form_inherit_website" model="ir.ui.view">
    <field name="name">res.lang.form.website.inherit</field>
    <field name="model">res.lang</field>
    <field name="inherit_id" ref="base.res_lang_form"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='url_code']" position="after">
            <field name="flag_image"/>
        </xpath>
    </field>
</record>

</odoo>
