<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_ras" name="Counter">
    <section class="s_count pt96 pb96" data-scroll-background-ratio="1">
        <div class="s_count">
        <div class="ml-5">Counter</div>
        <div class=" w-25 ml-5 p-3" >
            <div class="d-inline-block h1 plus" style="height:100px">+</div>
            <div class="d-inline-block ml-2 h3 result" style="height:100px">0</div>
            <div class="d-inline-block ml-2 h1 minus" style="height:100px">-</div>
        </div>
    </div>
    </section>
</template>

<template id="assets_snippet_s_ras_js_000" inherit_id="website.assets_frontend">
    <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/website/static/src/snippets/s_ras/000.js"/>
    </xpath>
</template>

</odoo>
