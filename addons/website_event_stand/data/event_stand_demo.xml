    <!--EVENT STAND FOR "Design Fair Los Angeles" EVENT-->

    <record id="event.event_0" model="event.event">
        <field name="website_menu" eval="True"/>
        <field name="website_stand" eval="True"/>
    </record>

    <record id="event_stand_0_event_0" model="event.stand">
        <field name="name">A1</field>
        <field name="event_id" ref="event.event_0"/>
        <field name="stand_type_id" ref="website_event_stand.event_stand_type_standard"/>
    </record>

    <record id="event_stand_0_event_0_slot_0" model="event.stand.slot">
        <field name="event_stand_id" ref="event_stand_0_event_0"/>
        <field name="date_from" model="event.stand.slot"
               eval="obj().env.ref('event.event_0').date_begin"/>
        <field name="date_to" model="event.stand.slot"
               eval="obj().env.ref('event.event_0').date_begin + relativedelta(hour=18)"/>
    </record>

    <record id="event_stand_0_event_0_slot_1" model="event.stand.slot">
        <field name="event_stand_id" ref="event_stand_0_event_0"/>
        <field name="date_from" model="event.stand.slot"
               eval="obj().env.ref('event.event_0').date_begin + timedelta(days=1)"/>
        <field name="date_to" model="event.stand.slot"
               eval="obj().env.ref('event.event_0').date_begin + relativedelta(days=1, hour=18)"/>
    </record>

