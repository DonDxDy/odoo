<?xml version="1.0" encoding="UTF-8" ?>
<odoo><data>

    <template id="assets_backend" inherit_id="web.assets_backend" name="Website Event Stand Assets Backend">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="website_event_stand/static/src/js/event_stand_configurator_controller.js"/>
            <script type="text/javascript" src="website_event_stand/static/src/js/event_stand_configurator_view.js"/>
            <script type="text/javascript" src="website_event_stand/static/src/js/event_stand_configurator_widget.js"/>
        </xpath>
    </template>

    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Slides Frontend Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/website_event_stand/static/src/js/stand_register.js"/>
        </xpath>
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/website_event_stand/static/src/scss/website_event_stand.scss" t-ignore="true"/>
        </xpath>
    </template>

</data></odoo>
