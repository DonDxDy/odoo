<?xml version="1.0" ?>
<odoo>
    <data>
        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Slides Frontend Assets">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/gamification_quiz/static/src/scss/gamification_quiz.scss" t-ignore="true"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/gamification_quiz/static/src/js/quiz.js"/>
                <script type="text/javascript" src="/gamification_quiz/static/src/js/quiz_finish.js"/>
                <script type="text/javascript" src="/gamification_quiz/static/src/js/quiz_question_form.js"/>
            </xpath>
        </template>
    </data>
</odoo>
